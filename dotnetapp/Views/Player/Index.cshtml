@model IEnumerable<Player>
<h2>List Of Players</h2>
 
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Age</th>
            <th>Category</th>
            <th>Bidding Amount</th>
            @* <th>TeamId</th>
            <th>Team Name</th> *@
 
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Category</td>
                <td>@item.BiddingAmount</td>
                @* <td>@item.TeamId</td>
                <td>@item.Team.TeamName</td> *@
                <td>@Html.ActionLink("Edit","Edit",new{id=item.Id})</td>
                <td>@Html.ActionLink("Delete","Delete",new{id=item.Id})</td>
 
            </tr>
        }
    </tbody>
</table>
 
@Html.ActionLink("Add Player","Add");
